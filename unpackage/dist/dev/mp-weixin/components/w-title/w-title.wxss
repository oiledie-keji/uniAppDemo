
.textContanier {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
}

/* 左边 */
.textContanierLeft {
	width: 70%;
	font-size: 36rpx;
	font-weight: 500;
	color: #333333;
}
.ticketHeader {
	padding: 16rpx 0;
	border-bottom: 1px solid rgba(0, 0, 0, 0.04);
}
.ticketHeaderTitle {
	color: rgba(0, 0, 0, 1);
}
.ticketHeaderPoint {
	font-size: 20rpx;
	color: rgba(102, 102, 102, 1);
}

/* 右边 */
.textContanierRight {
	width: 30%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: reverse;
	-webkit-flex-direction: row-reverse;
	        flex-direction: row-reverse;
}
.navigatorBox {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	
	-webkit-box-orient: vertical;
	
	-webkit-box-direction: normal;
	
	-webkit-flex-direction: column;
	
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.textContanierRight .textBox {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.textRight {
	line-height: 44rpx;
	font-size: 24rpx;
	color: #BBBBBB;
}
.arrowRight {
	width: 44rpx;
	height: 44rpx;
}

