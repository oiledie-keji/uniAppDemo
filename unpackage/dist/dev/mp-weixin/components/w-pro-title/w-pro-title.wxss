
.textContanier {
	position: relative;
}
.textLeft {
	font-size: 36rpx;
	font-weight: 500;
	position: relative;
	z-index: 1;
	color: #33333;
}
.textRight {
	font-size: 24rpx;
	color: #BBBBBB;
	float: right;
	position: absolute;
	top: 50%;
	right: 36rpx;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.arrowRight {
	width: 44rpx;
	height: 44rpx;
	position: absolute;
	top: 50%;
	right: -38rpx;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
}

